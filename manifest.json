{
    "manifest_version": 2,
    "name": "AI Assessment",
    "version": "1.0",
    "description": "Adds AI-powered answer suggestions to questions",
    "permissions": [
      "activeTab",
      "https://generativelanguage.googleapis.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }